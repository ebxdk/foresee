{
  "expo": {
    "name": "Forecee",
    "slug": "forecee",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/foreseelogoapp.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.capacitycreator.forecee",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "fetch"
        ],
        "LSApplicationQueriesSchemes": [
          "spotify",
          "spotify-action",
          "spotify-auth"
        ],
        "NSHealthShareUsageDescription": "This app reads your steps, heart data, sleep and mindfulness to personalize your EPC scores and wellness guidance.",
        "NSHealthUpdateUsageDescription": "This app writes health data only when you explicitly choose to log activities (not used currently)."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/foreseelogoapp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.capacitycreator.forecee"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/foreseelogoapp.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "react-native-health",
        {
          "healthSharePermission": "This app needs access to your health data to display your health metrics and statistics.",
          "healthUpdatePermission": "This app needs access to update your health data for better tracking and analysis."
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-web-browser",
      "./plugins/with-spotify-queries"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": { "projectId": "a97c70bc-c60e-4535-ac32-28bf3c0b269a" },
      "spotify": {
        "clientId": "3c81ab45f65d47b994709edae2595edf",
        "redirectPath": "spotify-auth"
      }
    },
    "owner": "ebxdk",
    "runtimeVersion": "1.0.0",
    "updates": { "url": "https://u.expo.dev/a97c70bc-c60e-4535-ac32-28bf3c0b269a" }
  }
}
